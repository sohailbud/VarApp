<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>WebApp</display-name>
  <servlet>
    <servlet-name>BarGraphServlet</servlet-name>
    <servlet-class>main.BarGraphServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>login.LogoutServlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>ModelDataServlet</servlet-name>
    <servlet-class>modeldata.ModelDataServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>BarGraphServlet</servlet-name>
    <url-pattern>/BarGraph</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>ModelDataServlet</servlet-name>
    <url-pattern>/ModelDataServlet</url-pattern>
  </servlet-mapping>
</web-app>